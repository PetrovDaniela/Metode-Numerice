#include <stdio.h>
#include <math.h>
#include <stdlib.h>

int i, j, n, a[100][100];
int main()
{
scanf("%d",&n);
for (i=1;i<=n;i++)
for (j=1;j<=n;j++)
{
printf("a[%d][%d]=",i,j);
scanf("%f",&a[i][j]);
}
for (j=1;j<=n;j++)
{
printf("a[%d][%d]=",1,j);
}
for (i=2;i<=n;i++)
{
printf("a[%d][%d]=",i,n);
}
for (j=n-1;j>=1;j--)

{
printf("a[%d][%d]=",n,j);
}

for (i=n-1;i>=2;i--)
{
printf("a[%d][%d]=",i,1);
}
}
