#include &lt;stdio.h&gt;
#include &lt;stdlib.h&gt;
int n,i,j,k,lin;
float a[100][100],b[101][101],x[100],S,piv,aux;
int main()
{
printf(&quot;dati n =&quot;);
scanf(&quot;%d&quot;,&amp;n);
for (i=1;i&lt;=n;i++)
for (j=1;j&lt;=n;j++)
{
printf(&quot;a[%d][%d]=&quot;,i,j);
scanf(&quot;%f&quot;,&amp;a[i][j]);
}
pas1:for (i=1;i&lt;=n;i++)
{
printf(&quot;b[%d][%d]=&quot;,i,n);
scanf(&quot;%f&quot;,&amp;b[i][n]);
}
for(j=n-1;j&gt;=1;j--)
for(i=1;i&lt;=n;i++)
{

b[i][j]=0;
for(k=1;k&lt;=n;k++)
b[i][j]=b[i][j]+a[i][k]*b[k][j+1];
}
for(i=1;i&lt;=n;i++)
{ b[i][n+1]=0;
for(k=1;k&lt;=n;k++)
b[i][n+1]=b[i][n+1]+a[i][k]*b[k][1];
b[i][n+1]=-b[i][n+1];
}
for (i=1;i&lt;=n;i++)
for (j=1;j&lt;=n+1;j++)
printf(&quot;b[%d][%d]=%f\n&quot;,i,j,b[i][j]);
for (k=1;k&lt;=n-1;k++)
{
piv=fabs(b[k][k]);
lin=k;
for (i=k+1;i&lt;=n;i++)
if (piv&lt;fabs(b[i][k]))
{
piv=fabs(b[i][k]);
lin=i;
}
if (piv==0)
{

goto pas1;
}
if(lin!=k)
for (j=k;j&lt;=n+1;j++)
{ aux=b[k][j];
b[k][j]=b[lin][j];
b[lin][j]=aux;
}
for (i=k+1;i&lt;=n;i++)
for (j=k+1;j&lt;=n+1;j++)
b[i][j]=(b[i][j]*b[k][k]-b[i][k]*b[k][j])/b[k][k];
}
x[n] = b[n][n+1]/b[n][n];
for (i=n-1;i&gt;=1;i--)
{ S=0;
for(j=i+1;j&lt;=n;j++)
S+=b[i][j]*x[j];
x[i]=(b[i][n+1]-S)/b[i][i];}
for (i=1;i&lt;=n;i++)
{
printf(&quot;X%d= %f&quot;, i, x[i]);
printf(&quot;\n&quot;);
}
}
